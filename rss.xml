---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
  <channel>
    <title>Alieniloquent</title>
    <link>http://blog.alieniloquent.com</link>
    <language>en</language>
    <pubDate>{{ site.time | date_to_xmlschema }}</pubDate>
    <copyright>Copyright 2005-2011</copyright>
    <ttl>60</ttl>
    <description>off on a tangent</description>
    {% for post in site.rss_posts limit:10 %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://blog.alieniloquent.com{{ post.url }}</link>
      <pubDate>{{ post.date }}</pubDate>
      <guid>http://blog.alieniloquent.com{{ post.url }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}
  </channel>
</rss>